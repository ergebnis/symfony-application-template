{
  "name": "ergebnis/symfony-application-template",
  "type": "project",
  "description": "Provides a GitHub repository template for a PHP library, using GitHub actions.",
  "homepage": "https://github.com/ergebnis/symfony-application-template",
  "license": "MIT",
  "authors": [
    {
      "name": "Andreas MÃ¶ller",
      "email": "am@localheinz.com"
    }
  ],
  "require": {
    "php": "^7.4.3",
    "ext-ctype": "*",
    "ext-iconv": "*",
    "symfony/config": "^5.0.7",
    "symfony/console": "^5.0.7",
    "symfony/dependency-injection": "^5.0.7",
    "symfony/dotenv": "^5.0.7",
    "symfony/error-handler": "^5.0.7",
    "symfony/flex": "^1.3.1",
    "symfony/framework-bundle": "^5.0.7",
    "symfony/http-kernel": "^5.0.7",
    "symfony/routing": "^5.0.7",
    "symfony/yaml": "^5.0.7"
  },
  "replace": {
    "paragonie/random_compat": "2.*",
    "symfony/polyfill-ctype": "*",
    "symfony/polyfill-iconv": "*",
    "symfony/polyfill-php56": "*",
    "symfony/polyfill-php70": "*",
    "symfony/polyfill-php71": "*",
    "symfony/polyfill-php72": "*",
    "symfony/polyfill-php73": "*",
    "symfony/polyfill-php74": "*"
  },
  "conflict": {
    "symfony/symfony": "*"
  },
  "require-dev": {
    "ergebnis/composer-normalize": "^2.4.0",
    "ergebnis/license": "~0.1.0",
    "ergebnis/php-cs-fixer-config": "^2.1.2",
    "ergebnis/phpstan-rules": "~0.14.4",
    "ergebnis/test-util": "~1.0.0",
    "infection/infection": "~0.16.3",
    "jangregor/phpstan-prophecy": "~0.6.2",
    "phpstan/extension-installer": "^1.0.4",
    "phpstan/phpstan": "~0.12.19",
    "phpstan/phpstan-deprecation-rules": "~0.12.2",
    "phpstan/phpstan-phpunit": "~0.12.8",
    "phpstan/phpstan-strict-rules": "~0.12.2",
    "phpunit/phpunit": "^9.1.3",
    "psalm/plugin-phpunit": "~0.10.0",
    "vimeo/psalm": "^3.10.1"
  },
  "config": {
    "platform": {
      "php": "7.4.3"
    },
    "preferred-install": "dist",
    "sort-packages": true
  },
  "extra": {
    "symfony": {
      "allow-contrib": false,
      "require": "5.0.*"
    }
  },
  "autoload": {
    "psr-4": {
      "Ergebnis\\Application\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Ergebnis\\Application\\Test\\": "test/"
    }
  },
  "scripts": {
    "post-install-cmd": [
      "@auto-scripts"
    ],
    "post-update-cmd": [
      "@auto-scripts"
    ],
    "auto-scripts": {
      "cache:clear": "symfony-cmd",
      "assets:install %PUBLIC_DIR%": "symfony-cmd"
    }
  },
  "support": {
    "issues": "https://github.com/ergebnis/symfony-application-template/issues",
    "source": "https://github.com/ergebnis/symfony-application-template"
  }
}
