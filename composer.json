{
  "name": "ergebnis/symfony-application-template",
  "description": "Provides a GitHub repository template for a PHP library, using GitHub actions.",
  "license": "MIT",
  "type": "project",
  "authors": [
    {
      "name": "Andreas MÃ¶ller",
      "email": "am@localheinz.com"
    }
  ],
  "homepage": "https://github.com/ergebnis/symfony-application-template",
  "support": {
    "issues": "https://github.com/ergebnis/symfony-application-template/issues",
    "source": "https://github.com/ergebnis/symfony-application-template"
  },
  "require": {
    "php": "^8.0",
    "ext-ctype": "*",
    "ext-iconv": "*",
    "doctrine/annotations": "^1.13.2",
    "doctrine/collections": "^1.6.8",
    "doctrine/doctrine-bundle": "^2.3.2",
    "doctrine/doctrine-migrations-bundle": "^3.2.1",
    "doctrine/migrations": "^3.3.2",
    "doctrine/orm": "^2.8.2",
    "ramsey/uuid": "^4.2.3",
    "sensio/framework-extra-bundle": "^6.2.1",
    "symfony/config": "^5.4.0",
    "symfony/console": "^5.4.1",
    "symfony/dependency-injection": "^5.4.0",
    "symfony/dotenv": "^6.0.1",
    "symfony/error-handler": "^6.0.1",
    "symfony/flex": "^2.0.1",
    "symfony/framework-bundle": "^5.4.1",
    "symfony/http-kernel": "^6.0.0",
    "symfony/routing": "^6.0.1",
    "symfony/yaml": "^6.0.1"
  },
  "require-dev": {
    "dama/doctrine-test-bundle": "^6.7.1",
    "ergebnis/composer-normalize": "^2.18.0",
    "ergebnis/factory-bot": "~0.4.0",
    "ergebnis/license": "^1.1.0",
    "ergebnis/php-cs-fixer-config": "^3.4.0",
    "ergebnis/test-util": "^1.5.0",
    "infection/infection": "~0.25.4",
    "phpunit/phpunit": "^9.5.10",
    "psalm/plugin-phpunit": "~0.16.1",
    "psalm/plugin-symfony": "^3.1.0",
    "symplify/config-transformer": "^10.0.2",
    "vimeo/psalm": "^4.14.0"
  },
  "replace": {
    "paragonie/random_compat": "2.*",
    "symfony/polyfill-ctype": "*",
    "symfony/polyfill-iconv": "*",
    "symfony/polyfill-php56": "*",
    "symfony/polyfill-php70": "*",
    "symfony/polyfill-php71": "*",
    "symfony/polyfill-php72": "*",
    "symfony/polyfill-php73": "*",
    "symfony/polyfill-php74": "*"
  },
  "conflict": {
    "symfony/symfony": "*"
  },
  "autoload": {
    "psr-4": {
      "App\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "App\\Test\\": "test/"
    }
  },
  "config": {
    "platform": {
      "php": "8.0.12"
    },
    "preferred-install": "dist",
    "sort-packages": true
  },
  "extra": {
    "symfony": {
      "allow-contrib": false,
      "require": "^5.1.0"
    }
  },
  "scripts": {
    "post-install-cmd": [
      "@auto-scripts"
    ],
    "post-update-cmd": [
      "@auto-scripts"
    ],
    "auto-scripts": {
      "cache:clear": "symfony-cmd",
      "assets:install %PUBLIC_DIR%": "symfony-cmd"
    }
  }
}
