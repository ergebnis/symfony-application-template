{
  "name": "ergebnis/symfony-application-template",
  "description": "Provides a GitHub repository template for a Symfony project, using GitHub Actions.",
  "license": "MIT",
  "type": "project",
  "authors": [
    {
      "name": "Andreas MÃ¶ller",
      "email": "am@localheinz.com"
    }
  ],
  "homepage": "https://github.com/ergebnis/symfony-application-template",
  "support": {
    "issues": "https://github.com/ergebnis/symfony-application-template/issues",
    "source": "https://github.com/ergebnis/symfony-application-template"
  },
  "require": {
    "php": "~8.2.0",
    "ext-ctype": "*",
    "ext-iconv": "*",
    "ext-mbstring": "*",
    "ext-pdo": "*",
    "ext-pdo_mysql": "*",
    "doctrine/annotations": "^2.0.1",
    "doctrine/collections": "^2.1.3",
    "doctrine/doctrine-bundle": "^2.10.2",
    "doctrine/doctrine-migrations-bundle": "^3.2.4",
    "doctrine/migrations": "^3.6.0",
    "doctrine/orm": "^2.16.2",
    "ramsey/uuid": "^4.7.4",
    "symfony/config": "^6.2.5",
    "symfony/console": "^6.3.4",
    "symfony/dependency-injection": "^6.3.1",
    "symfony/dotenv": "^6.3.0",
    "symfony/error-handler": "^6.3.0",
    "symfony/flex": "^2.3.3",
    "symfony/framework-bundle": "^6.3.4",
    "symfony/http-kernel": "^6.3.1",
    "symfony/routing": "^6.3.1",
    "symfony/runtime": "^6.3.2",
    "symfony/yaml": "^6.3.3"
  },
  "require-dev": {
    "dama/doctrine-test-bundle": "^7.2.1",
    "ergebnis/composer-normalize": "^2.36.0",
    "ergebnis/factory-bot": "^1.4.0",
    "ergebnis/license": "^2.2.0",
    "ergebnis/php-cs-fixer-config": "^5.15.1",
    "ergebnis/phpunit-slow-test-detector": "^2.3.0",
    "fakerphp/faker": "^1.23.0",
    "infection/infection": "~0.27.0",
    "phpunit/phpunit": "^10.3.4",
    "psalm/plugin-phpunit": "~0.18.4",
    "psalm/plugin-symfony": "^5.0.3",
    "rector/rector": "~0.18.2",
    "symplify/config-transformer": "^12.0.5",
    "vimeo/psalm": "^5.15.0"
  },
  "replace": {
    "paragonie/random_compat": "*",
    "symfony/polyfill-ctype": "*",
    "symfony/polyfill-iconv": "*",
    "symfony/polyfill-mbstring": "*",
    "symfony/polyfill-php56": "*",
    "symfony/polyfill-php70": "*",
    "symfony/polyfill-php71": "*",
    "symfony/polyfill-php72": "*",
    "symfony/polyfill-php73": "*",
    "symfony/polyfill-php74": "*",
    "symfony/polyfill-php80": "*",
    "symfony/polyfill-php81": "*"
  },
  "conflict": {
    "symfony/symfony": "*"
  },
  "autoload": {
    "psr-4": {
      "App\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "App\\Test\\": "test/"
    }
  },
  "config": {
    "allow-plugins": {
      "composer/package-versions-deprecated": true,
      "ergebnis/composer-normalize": true,
      "infection/extension-installer": true,
      "symfony/flex": true,
      "symfony/runtime": true
    },
    "platform": {
      "php": "8.2.0"
    },
    "preferred-install": "dist",
    "sort-packages": true
  },
  "extra": {
    "symfony": {
      "allow-contrib": false,
      "require": "^6.0.0"
    }
  },
  "scripts": {
    "post-install-cmd": [
      "@auto-scripts"
    ],
    "post-update-cmd": [
      "@auto-scripts"
    ],
    "auto-scripts": {
      "cache:clear": "symfony-cmd",
      "assets:install %PUBLIC_DIR%": "symfony-cmd"
    }
  }
}
